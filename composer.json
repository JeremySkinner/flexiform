  "type": "drupal-module",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "php": "^7.0",
    "drupal/core": "^8.5",
    "drupal/ctools": "~3.0-beta1"
